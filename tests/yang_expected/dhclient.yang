        container do-forward-updates {
          augex:data-path "do-forward-updates";
          augex:value-yang-path "sto_to_spc";
          presence "Type of config entry.";
          leaf sto_to_spc {
            mandatory true;
            type string {
              pattern "[^\\#,;{}\" \t\n]+|\"[^\\#\"\n]+\"";
            }
