            }
          }
        }
        container reboot {
          augex:data-path "reboot";
          augex:value-yang-path "sto_to_spc";
          presence "Type of config entry.";
          leaf sto_to_spc {
            mandatory true;
            type string {
              pattern "[^\\#,;{}\" \t\n]+|\"[^\\#\"\n]+\"";
            }
          }
        }
        container backoff-cutoff {
          augex:data-path "backoff-cutoff";
          augex:value-yang-path "sto_to_spc";
          presence "Type of config entry.";
          leaf sto_to_spc {
            mandatory true;
            type string {
              pattern "[^\\#,;{}\" \t\n]+|\"[^\\#\"\n]+\"";
            }
          }
        }
        container initial-interval {
          augex:data-path "initial-interval";
          augex:value-yang-path "sto_to_spc";
          presence "Type of config entry.";
          leaf sto_to_spc {
