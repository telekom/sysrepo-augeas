module aptcacherngsecurity {
  namespace "aug:aptcacherngsecurity";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  container aptcacherngsecurity {
    list record {
      key "_id";
      augex:data-path "##AdminAuth";
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key representing the position in augeas data.";
      }
      leaf authpair {
        mandatory true;
        type string {
          pattern "[^ \t:/]*";
        }
        augex:data-path "AdminAuth/$$authpair";
      }
      leaf value {
        mandatory true;
        type string {
          pattern "[^: \t\n]*";
        }
        augex:data-path "AdminAuth/^^authpair";
      }
    }
  }
}