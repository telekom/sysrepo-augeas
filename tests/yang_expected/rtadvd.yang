module rtadvd {
  namespace "aug:rtadvd";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  container rtadvd {
    list record {
      key "_id";
      augex:data-path "##record";
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key representing the position in augeas data.";
      }
      leaf-list name {
        min-elements 1;
        type string {
          pattern "[^#:\\\\\t\n|][^:\\\\\t\n|]*";
        }
        augex:data-path "record/name";
      }
      leaf-list capability {
        type string {
          pattern "[a-zA-Z0-9-]+(#?@|#[0-9]+|=(\"[^\"]*\"|[^:\"]*))?";
        }
        augex:data-path "record/capability";
      }
    }
  }
}