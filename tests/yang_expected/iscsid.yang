module iscsid {
  namespace "aug:iscsid";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  augex:augeas-mod-name "Iscsid";

  list iscsid {
    key "config-file";
    leaf config-file {
      type string;
    }
    list config-entries {
      key "_id";
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key to maintain the order of the augeas data.";
      }
      container kv {
        augex:data-path "$$";
        augex:value-yang-path "value_re";
        leaf key_re {
          mandatory true;
          type string {
            pattern "[][A-Za-z0-9_.-]+";
          }
        }
        leaf value_re {
          mandatory true;
          type string {
            pattern "[^ \t\n](.*[^ \t\n])?";
          }
        }
      }
    }
  }
}
