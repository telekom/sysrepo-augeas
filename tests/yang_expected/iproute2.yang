module iproute2 {
  namespace "aug:iproute2";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  augex:augeas-mod-name "IPRoute2";

  list iproute2 {
    key "config-file";
    leaf config-file {
      type string;
    }
    list record {
      key "id";
      augex:data-path "$$";
      augex:value-yang-path "value";
      leaf id {
        type string {
          pattern "(0x[0-9a-fA-F]+)|([0-9]+)";
        }
      }
      leaf value {
        mandatory true;
        type string {
          pattern "[a-zA-Z0-9/-]+";
        }
      }
    }
  }
}