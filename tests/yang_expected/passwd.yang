module passwd {
  namespace "aug:passwd";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  augex:augeas-mod-name "Passwd";

  list passwd {
    key "config-file";
    leaf config-file {
      type string;
    }
    list config-entries {
      key "_id";
      ordered-by user;
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key to maintain the order of the augeas data.";
      }
      choice ch_config-entries {
        mandatory true;
        container entry {
          augex:data-path "$$";
          presence "Type of config entry.";
          leaf username {
            mandatory true;
            type string {
              pattern "[_.A-Za-z0-9][-_.A-Za-z0-9]*";
            }
          }
          leaf password {
            mandatory true;
            type union {
              type empty;
              type string {
                pattern "[^:\n]+";
              }
            }
            augex:data-path "password";
          }
          leaf uid {
            mandatory true;
            type uint64;
            augex:data-path "uid";
          }
          leaf gid {
            mandatory true;
            type uint64;
            augex:data-path "gid";
          }
          leaf name {
            mandatory true;
            type union {
              type empty;
              type string {
                pattern "[^:\n]+";
              }
            }
            augex:data-path "name";
          }
          leaf home {
            mandatory true;
            type union {
              type empty;
              type string {
                pattern "[^:\n]+";
              }
            }
            augex:data-path "home";
          }
          leaf shell {
            mandatory true;
            type union {
              type empty;
              type string {
                pattern "[^ \t\n].*[^ \t\n]|[^ \t\n]";
              }
            }
            augex:data-path "shell";
          }
        }
        container nisentry {
          augex:data-path "@nis";
          augex:value-yang-path "username";
          presence "Type of config entry.";
          leaf username {
            mandatory true;
            type string {
              pattern "[_.A-Za-z0-9][-_.A-Za-z0-9]*";
            }
          }
          leaf password {
            type string {
              pattern "[^:\n]+";
            }
            augex:data-path "password";
          }
          leaf uid {
            type uint64;
            augex:data-path "uid";
          }
          leaf gid {
            type uint64;
            augex:data-path "gid";
          }
          leaf name {
            type string {
              pattern "[^:\n]+";
            }
            augex:data-path "name";
          }
          leaf home {
            type string {
              pattern "[^:\n]+";
            }
            augex:data-path "home";
          }
          leaf shell {
            type string {
              pattern "[^ \t\n].*[^ \t\n]|[^ \t\n]";
            }
            augex:data-path "shell";
          }
        }
        container nisdefault {
          augex:data-path "@nisdefault";
          presence "Type of config entry.";
          leaf password {
            type string {
              pattern "[^:\n]*";
            }
            augex:data-path "password";
          }
          leaf uid {
            type union {
              type string {
                length 0;
              }
              type uint64;
            }
            augex:data-path "uid";
          }
          leaf gid {
            type union {
              type string {
                length 0;
              }
              type uint64;
            }
            augex:data-path "gid";
          }
          leaf name {
            type union {
              type empty;
              type string {
                pattern "[^:\n]+";
              }
            }
            augex:data-path "name";
          }
          leaf home {
            type union {
              type empty;
              type string {
                pattern "[^:\n]+";
              }
            }
            augex:data-path "home";
          }
          leaf shell {
            type union {
              type empty;
              type string {
                pattern "[^ \t\n].*[^ \t\n]|[^ \t\n]";
              }
            }
            augex:data-path "shell";
          }
        }
        container nisuserplus {
          augex:data-path "@+nisuser";
          augex:value-yang-path "username";
          presence "Type of config entry.";
          leaf username {
            mandatory true;
            type string {
              pattern "[_.A-Za-z0-9][-_.A-Za-z0-9]*";
            }
          }
          leaf password {
            type string {
              pattern "[^:\n]+";
            }
            augex:data-path "password";
          }
          leaf uid {
            type uint64;
            augex:data-path "uid";
          }
          leaf gid {
            type uint64;
            augex:data-path "gid";
          }
          leaf name {
            type string {
              pattern "[^:\n]+";
            }
            augex:data-path "name";
          }
          leaf home {
            type string {
              pattern "[^:\n]+";
            }
            augex:data-path "home";
          }
          leaf shell {
            type string {
              pattern "[^ \t\n].*[^ \t\n]|[^ \t\n]";
            }
            augex:data-path "shell";
          }
        }
        container nisuserminus {
          augex:data-path "@-nisuser";
          augex:value-yang-path "username";
          presence "Type of config entry.";
          leaf username {
            mandatory true;
            type string {
              pattern "[_.A-Za-z0-9][-_.A-Za-z0-9]*";
            }
          }
          leaf password {
            type string {
              pattern "[^:\n]+";
            }
            augex:data-path "password";
          }
          leaf uid {
            type uint64;
            augex:data-path "uid";
          }
          leaf gid {
            type uint64;
            augex:data-path "gid";
          }
          leaf name {
            type string {
              pattern "[^:\n]+";
            }
            augex:data-path "name";
          }
          leaf home {
            type string {
              pattern "[^:\n]+";
            }
            augex:data-path "home";
          }
          leaf shell {
            type string {
              pattern "[^ \t\n].*[^ \t\n]|[^ \t\n]";
            }
            augex:data-path "shell";
          }
        }
      }
    }
  }
}
