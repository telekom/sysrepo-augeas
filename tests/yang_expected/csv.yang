module csv {
  namespace "aug:csv";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  augex:augeas-mod-name "CSV";

  list csv {
    key "config-file";
    min-elements 1;
    leaf config-file {
      type string;
    }
    list entry {
      key "_id";
      augex:data-path "$$";
      leaf _id {
        type uint64;
      }
      list field {
        key "_id";
        min-elements 1;
        augex:data-path "$$/$$";
        augex:value-yang-path "value";
        leaf _id {
          type uint64;
        }
        leaf value {
          type string {
            pattern "([^\n\"#;])*";
          }
        }
      }
      list field2 {
        key "_id";
        min-elements 1;
        augex:data-path "$$/$$";
        augex:value-yang-path "value";
        leaf _id {
          type uint64;
        }
        leaf value {
          type string {
            pattern "(\"[^\"#]*\")+";
          }
        }
      }
    }
  }
}