module csv {
  namespace "aug:csv";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  augex:augeas-mod-name "CSV";

  list csv {
    key "config-file";
    leaf config-file {
      type string;
    }
    list entry {
      key "_id";
      augex:data-path "$$";
      leaf _id {
        type uint64;
      }
      choice ch_entry {
        case field {
          leaf-list field {
            min-elements 1;
            type string {
              pattern "([^\n\"#;])*";
            }
            augex:data-path "$$/$AY_UINT";
          }
        }
        case field2 {
          leaf-list field2 {
            min-elements 1;
            type string {
              pattern "(\"[^\"#]*\")+";
            }
            augex:data-path "$$/$AY_UINT";
          }
        }
      }
    }
  }
}