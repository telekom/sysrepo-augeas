module csv {
  namespace "aug:csv";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  container csv {
    list entry {
      key "_id";
      augex:data-path "$$_id";
      leaf _id {
        type uint64;
      }
      choice ch_entry {
        case field {
          leaf-list field {
            min-elements 1;
            type string {
              pattern "([^\n\"#;])*";
            }
            augex:data-path "$$_id/$AY_UINT";
          }
        }
        case field2 {
          leaf-list field2 {
            min-elements 1;
            type string {
              pattern "(\"[^\"#]*\")+";
            }
            augex:data-path "$$_id/$AY_UINT";
          }
        }
      }
    }
  }
}