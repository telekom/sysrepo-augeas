module login_defs {
  namespace "aug:login_defs";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  augex:augeas-mod-name "Login_defs";

  list login_defs {
    key "config-file";
    leaf config-file {
      type string;
    }
    list record {
      key "_id";
      augex:data-path "$$";
      augex:value-yang-path "value";
      leaf _id {
        type string {
          pattern "[A-Za-z0-9_.-]+";
        }
      }
      leaf value {
        mandatory true;
        type string {
          pattern "[^ \t\n]+([ \t]+[^ \t\n]+)*";
        }
      }
    }
  }
}