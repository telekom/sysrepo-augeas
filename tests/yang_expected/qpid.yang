module qpid {
  namespace "aug:qpid";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  container qpid {
    list entry {
      key "key";
      augex:data-path "$key";
      leaf key {
        type string {
          pattern "[A-Za-z0-9_.-]+";
        }
      }
      leaf node {
        type string {
          pattern "[^ \t\n].*[^ \t\n]|[^ \t\n]";
        }
        augex:data-path "^key";
      }
    }
  }
}