module gtkbookmarks {
  namespace "aug:gtkbookmarks";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  augex:augeas-mod-name "GtkBookmarks";

  list gtkbookmarks {
    key "config-file";
    min-elements 1;
    leaf config-file {
      type string;
    }
    list bookmark {
      key "bookmark";
      augex:data-path "bookmark";
      leaf bookmark {
        type string {
          pattern "[^ \t\n]+";
        }
      }
      leaf label {
        type string {
          pattern "[^ \t\n].*[^ \t\n]|[^ \t\n]";
        }
        augex:data-path "bookmark/label";
      }
    }
  }
}