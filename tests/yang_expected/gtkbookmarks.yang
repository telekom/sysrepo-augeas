module gtkbookmarks {
  namespace "aug:gtkbookmarks";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  augex:augeas-mod-name "GtkBookmarks";

  list gtkbookmarks {
    key "config-file";
    leaf config-file {
      type string;
    }
    list config-entries {
      key "_id";
      ordered-by user;
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key to maintain the order of the augeas data.";
      }
      container entry {
        augex:data-path "bookmark";
        augex:value-yang-path "bookmark";
        presence "Config entry.";
        leaf bookmark {
          mandatory true;
          type string {
            pattern "[^ \t\n]+";
          }
        }
        leaf label {
          type string {
            pattern "[^ \t\n].*[^ \t\n]|[^ \t\n]";
          }
          augex:data-path "label";
        }
      }
    }
  }
}
