              key "_id";
              ordered-by user;
              leaf _id {
                type uint64;
                description
                  "Implicitly generated list key to maintain the order of the augeas data.";
              }
              uses entry;
            }
            choice ch-case2 {
              container case {
                presence "Config entry.";
                uses baseurl-list;
                list entry-list {
                  key "_id";
                  ordered-by user;
                  leaf _id {
                    type uint64;
                    description
                      "Implicitly generated list key to maintain the order of the augeas data.";
                  }
                  uses entry;
                }
                uses exclude-list;
                list entry-list2 {
                  key "_id";
                  ordered-by user;
                  leaf _id {
                    type uint64;
                    description
                      "Implicitly generated list key to maintain the order of the augeas data.";
                  }
                  uses entry;
                }
              }
              container case2 {
                presence "Config entry.";
                uses exclude-list;
                list entry-list {
                  key "_id";
                  ordered-by user;
                  leaf _id {
                    type uint64;
                    description
                      "Implicitly generated list key to maintain the order of the augeas data.";
                  }
                  uses entry;
                }
                uses baseurl-list;
                list entry-list2 {
                  key "_id";
                  ordered-by user;
                  leaf _id {
                    type uint64;
                    description
                      "Implicitly generated list key to maintain the order of the augeas data.";
                  }
                  uses entry;
                }
              }
            }
          }
          container case3 {
            presence "Config entry.";
            uses exclude-list;
            list entry-list {
              key "_id";
              ordered-by user;
              leaf _id {
                type uint64;
                description
                  "Implicitly generated list key to maintain the order of the augeas data.";
              }
              uses entry;
            }
            choice ch-case3 {
              container case {
                presence "Config entry.";
                uses gpgkey-list;
                list entry-list {
                  key "_id";
                  ordered-by user;
                  leaf _id {
                    type uint64;
                    description
                      "Implicitly generated list key to maintain the order of the augeas data.";
                  }
                  uses entry;
                }
                uses baseurl-list;
                list entry-list2 {
                  key "_id";
                  ordered-by user;
                  leaf _id {
                    type uint64;
                    description
                      "Implicitly generated list key to maintain the order of the augeas data.";
                  }
                  uses entry;
                }
              }
              container case2 {
                presence "Config entry.";
                uses baseurl-list;
                list entry-list {
                  key "_id";
                  ordered-by user;
                  leaf _id {
                    type uint64;
                    description
                      "Implicitly generated list key to maintain the order of the augeas data.";
                  }
                  uses entry;
                }
                uses gpgkey-list;
                list entry-list2 {
                  key "_id";
                  ordered-by user;
                  leaf _id {
                    type uint64;
                    description
                      "Implicitly generated list key to maintain the order of the augeas data.";
                  }
                  uses entry;
                }
              }
            }
          }
        }
      }
    }
  }
}
