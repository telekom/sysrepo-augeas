module vmware_config {
  namespace "aug:vmware_config";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  container vmware_config {
    list entry {
      key "key";
      augex:data-path "$key";
      leaf key {
        type string {
          pattern "[A-Za-z0-9_.-]+";
        }
      }
      leaf node {
        type string {
          pattern "[^\n\t \"]([^\n\"]*[^\n\t \"])?";
        }
        augex:data-path "$key";
      }
    }
  }
}