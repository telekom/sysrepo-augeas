module cmdline {
  namespace "aug:cmdline";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  augex:augeas-mod-name "Cmdline";

  container cmdline {
    list list {
      key "_id";
      augex:data-path "$$_id";
      leaf _id {
        type string {
          pattern "[A-Za-z0-9_.-]+";
        }
      }
      leaf value {
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "^^_id";
      }
    }
  }
}