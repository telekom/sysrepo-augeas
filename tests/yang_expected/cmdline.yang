module cmdline {
  namespace "aug:cmdline";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  container cmdline {
    list list {
      key "key";
      leaf key {
        type string {
          pattern "[A-Za-z0-9_.-]+";
        }
        augex:data-path "$key";
      }
      leaf node {
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "$key";
      }
    }
  }
}