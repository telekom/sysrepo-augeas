module rmt {
  namespace "aug:rmt";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  container rmt {
    list debug {
      key "_id";
      augex:data-path "##DEBUG";
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key representing the position in augeas data.";
      }
      leaf value {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "^^DEBUG";
      }
    }
    list user {
      key "_id";
      augex:data-path "##USER";
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key representing the position in augeas data.";
      }
      leaf sto_to_tab {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "^^USER";
      }
    }
    list access {
      key "_id";
      augex:data-path "##ACCESS";
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key representing the position in augeas data.";
      }
      leaf name {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "ACCESS/name";
      }
      leaf host {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "ACCESS/host";
      }
      leaf path {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "ACCESS/path";
      }
    }
  }
}