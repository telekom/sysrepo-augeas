module rmt {
  namespace "aug:rmt";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  augex:augeas-mod-name "Rmt";

  list rmt {
    key "config-file";
    min-elements 1;
    leaf config-file {
      type string;
    }
    list DEBUG {
      key "_id";
      augex:data-path "##DEBUG";
      augex:value-yang-path "value";
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key representing the position in augeas data.";
      }
      leaf value {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
      }
    }
    list USER {
      key "_id";
      augex:data-path "##USER";
      augex:value-yang-path "sto_to_tab";
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key representing the position in augeas data.";
      }
      leaf sto_to_tab {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
      }
    }
    list ACCESS {
      key "_id";
      augex:data-path "##ACCESS";
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key representing the position in augeas data.";
      }
      leaf name {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "ACCESS/name";
      }
      leaf host {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "ACCESS/host";
      }
      leaf path {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "ACCESS/path";
      }
    }
  }
}