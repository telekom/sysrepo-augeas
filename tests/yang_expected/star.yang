module star {
  namespace "aug:star";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  augex:augeas-mod-name "Star";

  list star {
    key "config-file";
    min-elements 1;
    leaf config-file {
      type string;
    }
    list STAR_FIFOSIZE {
      key "_id";
      augex:data-path "##STAR_FIFOSIZE";
      augex:value-yang-path "value";
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key representing the position in augeas data.";
      }
      leaf value {
        mandatory true;
        type string {
          pattern "[0-9x*.a-z]+";
        }
      }
    }
    list STAR_FIFOSIZE_MAX {
      key "_id";
      augex:data-path "##STAR_FIFOSIZE_MAX";
      augex:value-yang-path "value";
      leaf _id {
        type uint64;
        description
          "Implicitly generated list key representing the position in augeas data.";
      }
      leaf value {
        mandatory true;
        type string {
          pattern "[0-9x*.a-z]+";
        }
      }
    }
    list archive {
      key "_id";
      augex:data-path "$$";
      leaf _id {
        type string {
          pattern "(archive)([0-7])";
        }
      }
      leaf device {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "$$/device";
      }
      leaf block {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "$$/block";
      }
      leaf size {
        mandatory true;
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "$$/size";
      }
      leaf istape {
        type string {
          pattern "[^ \t\n]+";
        }
        augex:data-path "$$/istape";
      }
    }
  }
}