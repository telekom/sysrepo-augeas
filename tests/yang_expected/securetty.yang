module securetty {
  namespace "aug:securetty";
  prefix aug;

  import augeas-extension {
    prefix augex;
  }

  container securetty {
    list record {
      key "_id";
      augex:data-path "$_id";
      leaf _id {
        type uint64;
      }
      leaf word {
        type string {
          pattern "[^ \t\n#]+";
        }
        augex:data-path "^_id";
      }
    }
  }
}